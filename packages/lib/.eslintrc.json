{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "**/SystemProto.ts"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["**/tests/**/*.ts"],
      "rules": {
        "@typescript-eslint/no-non-null-assertion": ["off"],
        "@typescript-eslint/no-unused-vars": ["off"],
        "@typescript-eslint/no-var-requires": ["off"]
      }
    }
  ]
}
